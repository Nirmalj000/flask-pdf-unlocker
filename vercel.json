{
  "version": 2,
  "builds": [
    {
      "src": "app/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/Templates/(.*)",
      "dest": "/app/Templates/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/app/app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py"
  }
}
